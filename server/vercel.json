{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "/socket.io/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }