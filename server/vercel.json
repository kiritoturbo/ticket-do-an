{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/socket.io/**",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }