{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/**",
        "status": 308,
        "headers": { "Location": "https://ticket-truong-client.web.app" },
        "dest": "/socket.io/**"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }